<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	default-init-method="init" default-destroy-method="destroy"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">





	<bean id="logger" class="com.caveofprogramming.spring.test.Logger"
		autowire="byName">

	</bean>

	<bean name="consoleWriter" class="com.caveofprogramming.spring.test.ConsoleWriter">
	</bean>


	<bean name="fileWriter" class="com.caveofprogramming.spring.test.FileWriter">
	</bean>
</beans>
